<mapper namespace="com.trkj.train.mapper.SysDeptMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.trkj.train.entity.SysDept">
        <id column="DEPT_ID" property="deptId" />
