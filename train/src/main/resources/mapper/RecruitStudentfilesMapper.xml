        <result column="STUDENTFILES_NAME" property="studentfilesName" />
        <result column="STUDENTFILES_SEX" property="studentfilesSex" />
        <result column="STUDENTFILES_AGE" property="studentfilesAge" />
        <result column="STUDENTFILES_BIRTHDAY" property="studentfilesBirthday" />
        <result column="STUDENTFILES_PHONE" property="studentfilesPhone" />
        <result column="PARENT_PHONE" property="parentPhone" />
        <result column="STUDENTFILES_SCHOOL" property="studentfilesSchool" />
        <result column="STUDENTFILES_LOC" property="studentfilesLoc" />
        <result column="STUDENTFILES_REMARKS" property="studentfilesRemarks" />
        <result column="CHANNEL_ID" property="channelId" />
        <result column="STUDENTFILES_STATE" property="studentfilesState" />
        <result column="COURSE_ID" property="courseId" />
        <result column="DELETED" property="deleted" />
    </resultMap>
    <resultMap id="ConsultationResultMap" type="com.trkj.train.TyVo.ConsultationDo">
        <result column="studentFiles_id" property="studentFiles_id"/>
        <result column="studentFiles_name" property="studentFiles_name" />
        <result column="studentFiles_sex" property="studentFiles_sex" />
        <result column="studentFiles_age" property="studentFiles_age" />
        <result column="studentFiles_phone" property="studentFiles_phone" />
        <result column="studentFiles_birthday" property="studentFiles_birthday"/>
        <result column="parent_phone" property="parent_phone"/>
        <result column="studentFiles_school" property="studentFiles_school" />
        <result column="studentFiles_loc" property="studentFiles_loc" />
        <result column="studentFiles_remarks" property="studentFiles_remarks" />
        <result column="studentFiles_state" property="studentFiles_state" />
        <result column="course_name" property="course_name" />
