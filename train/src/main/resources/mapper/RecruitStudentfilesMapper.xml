        <result column="STAFF_NAME" property="staffName" />
        <result column="staff_id" property="staffId"/>
    </resultMap>
    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        STUDENTFILES_ID, STUDENTFILES_NAME, STUDENTFILES_SEX, STUDENTFILES_AGE, STUDENTFILES_BIRTHDAY, STUDENTFILES_PHONE, PARENT_PHONE, STUDENTFILES_SCHOOL, STUDENTFILES_LOC, STUDENTFILES_REMARKS, CHANNEL_ID, STUDENTFILES_STATE, COURSE_ID, DELETED
    </sql>
    <select id="paging" resultMap="ConsultationResultMap">
        SELECT * FROM recruit_studentFiles s
                          LEFT JOIN sys_staff a ON s.STAFF_ID = a.STAFF_ID
                          LEFT JOIN recruit_course c ON s.course_id = c.course_id
    </select>
    <select id="paging1" resultMap="ConsultationResultMap">
        SELECT * FROM recruit_studentFiles s
                            LEFT JOIN sys_staff a ON s.STAFF_ID = a.STAFF_ID
                            LEFT JOIN recruit_course c ON s.course_id = c.course_id
            ${ew.customSqlsegment}
    </select>
    <select id="selectipotion" resultMap="ConsultationResultMap">
        SELECT * FROM recruit_studentFiles s
                          LEFT JOIN sys_staff a ON s.STAFF_ID = a.STAFF_ID
                          LEFT JOIN recruit_course c ON s.course_id = c.course_id
            ${ew.customSqlsegment}
    </select>
<select id="pagfyetwo" resultMap="studentfilesqudfao">
    SELECT * FROM recruit_studentFiles s
        LEFT JOIN sys_staff a ON s.STAFF_ID = a.STAFF_ID WHERE channel_id=#{id} and STUDENTFILES_NAME like #{name}
</select>
    <select id="pagfye" resultMap="studentfilesqudfao">
        SELECT * FROM recruit_studentFiles s LEFT JOIN sys_staff a ON s.STAFF_ID = a.STAFF_ID WHERE channel_id=#{id}
    </select>
