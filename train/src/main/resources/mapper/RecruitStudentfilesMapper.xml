        <result column="staff_id" property="staff_id"/>
        <result column="studentFiles_time" property="studentFiles_time"/>
        <result column="staff_name" property="staff_name"/>
    </resultMap>
    <resultMap id="studentfilesqudfao" type="com.trkj.train.TyVo.StudentfilesQudaoDo">
        <result column="STUDENTFILES_ID" property="studentfilesId"/>
        <result column="studentFiles_name" property="studentfilesName" />
        <result column="studentFiles_sex" property="studentfilesSex" />
        <result column="studentFiles_age" property="studentfilesAge" />
